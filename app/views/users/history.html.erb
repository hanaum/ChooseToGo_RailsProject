<div class="container">
  <div class="row">
    <div class="col-sm-12 col-lg-12 text-center">
      <div class="col-sm-12 col-lg-12 text-center" id="listpage-div">
        <a class="homepage-link" href="/users">Back To List Page</a>
      </div>
      <h1 id="history-title">Your Travel History</h1>
      <div id="history-list" >
        <ul>
          <% @locations.each do |loc|%>
            <li class="history-list">
              <form action="/location/<%=loc.id%>" method="post">
                <input type='hidden' name='authenticity_token' value='<%= form_authenticity_token %>'>
                <input class="history-link" type="submit" value="<%=loc.name%>">
              </form>
            </li>
          <%end%>
        </ul>
      </div>
    </div>
  </div>
</div>

